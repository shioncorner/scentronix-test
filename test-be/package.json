{
  "name": "test-be",
  "version": "1.0.0",
  "description": "Node, express, postgres, typescript boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_PATH=./src tsnd --respawn ./src/index.ts",
    "start": "cross-env NODE_PATH=./dist node ./dist/index.js",
    "test": "jest",
    "build": "rimraf dist && tsc",
    "docker:dev": "docker-compose --file docker-compose.yml --file docker-compose.dev.yml up --build",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts"
  },
  "author": "Shion",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^9.1.0",
    "@types/jest": "^29.5.12",
    "axios": "^1.7.2",
    "axios-mock-adapter": "^1.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^7.4.3",
    "mongoose": "^6.0.8",
    "morgan": "^1.10.0",
    "ts-jest": "^29.1.5",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.4.3"
